{
	"jeebs": {
		"app": {
			"name": "PostgreSQL",
			"suite": "Jeebs"
		},
    "db": {
      "default": "arwen",
      "connections": {
        "arwen": {
          "connectionString": ""
        }
      }
    },
		"logging": {
			"minimum": "Verbose",
      "overrides": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "Npgsql.Command": "Information",
        "Npgsql.Connection": "Information",
        "Npgsql.Transaction": "Information"
      },
      "providers": {
        "console.local": {
          "enabled": true
        },
        "seq.live": {
          "enabled": true,
          "minimum": "Information"
        }
      }
		},
    "services": {
      "console": {
        "local": {}
      },
      "seq": {
        "live": {
          "server": "",
          "apiKey": ""
        }
      }
    }
	}
}